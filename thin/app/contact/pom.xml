<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

  <modelVersion>4.0.0</modelVersion>

  <parent>
    <groupId>org.jbundle.app.office</groupId>
    <artifactId>jbundle-office-thin-app-reactor</artifactId>
    <version>1.1.3-SNAPSHOT</version>
  </parent>

  <artifactId>org.jbundle.thin.app.office.contact.app</artifactId>
  <packaging>bundle</packaging>
  <name>jbundle-thin-app-office-contact-app</name>

  <properties>
    <mainClass>${project.artifactId}.ThinContactApplet</mainClass>
    <jbundle-package-import>
        *;resolution:=optional,
        org.jbundle.res.app.office.contact.db;resolution:=optional,
    </jbundle-package-import>
  </properties>

  <dependencies>
    <dependency>
      <groupId>org.jbundle.config.dep</groupId>
      <artifactId>jbundle-config-dep-thin</artifactId>
      <version>${jbundle-version-range}</version>
      <type>pom</type>
    </dependency>
    <dependency>
      <groupId>org.jbundle.app.office</groupId>
      <artifactId>org.jbundle.thin.app.office.contact.db</artifactId>
      <version>${jbundle-app-office-version-range}</version>
    </dependency>
    <dependency>
      <groupId>org.jbundle.app.office</groupId>
      <artifactId>org.jbundle.res.app.office.contact</artifactId>
      <version>${jbundle-app-office-version-range}</version>
    </dependency>
  </dependencies>

</project>
